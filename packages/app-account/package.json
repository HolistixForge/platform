{
  "name": "@monorepo/app-account",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "app-account",
    "projectType": "application",
    "sourceRoot": "packages/app-account/src",
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "platform": "node",
          "outputPath": "dist/packages/app-account",
          "format": [
            "cjs"
          ],
          "bundle": true,
          "thirdParty": true,
          "external": [
            "react",
            "react-dom"
          ],
          "main": "packages/app-account/src/main.ts",
          "tsConfig": "packages/app-account/tsconfig.app.json",
          "assets": [
            "packages/app-account/src/assets"
          ],
          "esbuildOptions": {
            "sourcemap": true,
            "outExtension": {
              ".js": ".js"
            }
          }
        },
        "configurations": {
          "development": {},
          "production": {
            "esbuildOptions": {
              "sourcemap": false,
              "outExtension": {
                ".js": ".js"
              }
            }
          }
        }
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "app-account:build",
          "runBuildTargetDependencies": true
        },
        "configurations": {
          "development": {
            "buildTarget": "app-account:build:development"
          },
          "production": {
            "buildTarget": "app-account:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  }
}
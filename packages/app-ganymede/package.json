{
  "name": "@monorepo/app-ganymede",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "nx": {
    "name": "app-ganymede",
    "projectType": "application",
    "sourceRoot": "packages/app-ganymede/src",
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "platform": "node",
          "outputPath": "dist/packages/app-ganymede",
          "format": [
            "esm"
          ],
          "bundle": true,
          "thirdParty": true,
          "external": [
            "react",
            "react-dom"
          ],
          "main": "packages/app-ganymede/src/main.mts",
          "tsConfig": "packages/app-ganymede/tsconfig.app.json",
          "assets": [
            "packages/app-ganymede/src/assets"
          ],
          "esbuildOptions": {
            "sourcemap": true,
            "outExtension": {
              ".js": ".mjs"
            }
          }
        },
        "configurations": {
          "development": {},
          "production": {
            "esbuildOptions": {
              "sourcemap": false,
              "outExtension": {
                ".js": ".mjs"
              }
            }
          }
        }
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "app-ganymede:build",
          "runBuildTargetDependencies": true
        },
        "configurations": {
          "development": {
            "buildTarget": "app-ganymede:build:development"
          },
          "production": {
            "buildTarget": "app-ganymede:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  }
}